          "enabled": true,
          "server_name": "",
          "utls": {
              "enabled": true,
              "fingerprint": "chrome"
          },
          "reality": {
              "enabled": true,
              "public_key": "",
              "short_id": ""
          }
      },
      "packet_encoding": "xudp"
    },
    {
      "tag":"proxy",
      "type":"selector",
      "outbounds":[
        "auto",
        "direct",
        "ss",
        "vless"
      ]
    },
    {
      "tag":"auto",
      "type":"urltest",
      "outbounds":[
        "ss",
        "vless"
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
